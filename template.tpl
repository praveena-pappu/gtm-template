// Require APIs
const injectScript = require('injectScript');
const copyFromWindow = require('copyFromWindow');
const setInWindow = require('setInWindow');
const getTimestamp = require('getTimestamp');
const callInWindow = require('callInWindow');
const createQueue = require('createQueue');
const makeInteger = require('makeInteger');

// Settings for pixel
const settings = {
    cid: data.clientID,
    eventType: data.eventType,
    conversionCategory: data.conversionCategory,
    revenue: data.conversionValue,
    partner: data.campaignID,
    eid: data.productID,
    lang: 'us',
    host: 'a.lentainform.com',
    nosafari: true,
    notTriggeredEvents: ['pageView', 'dynamicRetargeting'],
    onSuccess: data.gtmOnSuccess,
    onFailure: data.gtmOnFailure,
    getSensorUrl: () => 'https://' + settings.host + '/lentasensor.js?d=' + getTimestamp() + '&source=gtm',
};

/**
 * Lentainform Pixel
 * @type {{init: pixel.init, send: pixel.send, exec: pixel.exec}}
 */
const pixel = {
    /**
     * Sensor initialization
     * @param options {object}
     */
    init: function(options) {
        let lentaSensorDataPush = createQueue('lentaSensorData');
        lentaSensorDataPush({
            cid: makeInteger(options.cid),
            lng: options.lang,
            nosafari: options.nosafari,
            project: options.host,
            eid: options.eid,
            partner: makeInteger(options.partner),
        });
        injectScript(
            options.getSensorUrl(),
            options.onSuccess,
            options.onFailure
        );
        setInWindow('LentainformSensorInjected', 1);
    },

    /**
     * Send pixel
     * @param stage {string}
     * @param category {string}
     * @param revenue {number}
     */
    send: function(stage, category, revenue) {
        callInWindow(
            'lentaSensor.invoke',
            '__gtm_' +  stage + '_target',
            {
                gtm_stage: stage,
                gtm_category: category,
                gtm_revenue: revenue,
            }
        );
    },

    /**
     * Execute pixel
     * @param options {object}
     */
    exec: function(options) {
        if (!copyFromWindow('LentainformSensorInjected')) {
            pixel.init(options);
        }

        if (options.notTriggeredEvents.indexOf(options.eventType) !== -1) {
            options.onSuccess();
            return;
        }

        pixel.send(options.eventType, options.conversionCategory, options.revenue);
        options.onSuccess();
    },
};

// Executing Lentainform pixel
pixel.exec(settings);


